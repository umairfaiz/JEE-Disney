<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>accountServlet</servlet-name>
        <servlet-class>com.src.servlets.accountServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ticketServelt</servlet-name>
        <servlet-class>com.src.servlets.ticketServelt</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>eloadServlet</servlet-name>
        <servlet-class>com.src.servlets.eloadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ticketServlet</servlet-name>
        <servlet-class>com.src.servlets.ticketServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>e_loadServlet</servlet-name>
        <servlet-class>com.src.servlets.e_loadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>lockerServlet</servlet-name>
        <servlet-class>com.src.servlets.lockerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>accountServlet</servlet-name>
        <url-pattern>/accountServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ticketServelt</servlet-name>
        <url-pattern>/ticketServelt</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>eloadServlet</servlet-name>
        <url-pattern>/eloadServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ticketServlet</servlet-name>
        <url-pattern>/ticketServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>e_loadServlet</servlet-name>
        <url-pattern>/e_loadServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>lockerServlet</servlet-name>
        <url-pattern>/lockerServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
